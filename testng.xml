<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test thread-count="5" name="Test">
		<classes>
			<class name="Learning.Selenium.PracticeTestNg" />
		</classes>
	</test> <!-- Test -->

	<test name="Car Loan">
		<classes>
			<class name="Learning.Selenium.day1">
				<methods>
					<exclude name="MobileLoginCarLoan"></exclude>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Home Loan">
		<classes>
			<class name="Learning.Selenium.day2">
				<methods>
					<exclude name="webLoginHomeLoan"></exclude>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Personal Loan">
		<classes>
			<class name="Learning.Selenium.day3">
				<methods>
					<include name="APILogin.*" />
				</methods>
			</class>
		</classes>
	</test>


</suite> <!-- Suite -->